        <BeanShellSampler guiclass="BeanShellSamplerGui" testclass="BeanShellSampler" testname="BeanShell Sampler" enabled="true">
          <stringProp name="BeanShellSampler.query">import org.apache.jmeter.services.FileServer;
import org.apache.jmeter.util.JMeterUtils;
int failedResponseThreshold = Integer.valueOf(vars.get(&quot;failedResponseThreshold&quot;));
int averageResponseTimeThreshold = Integer.valueOf(vars.get(&quot;averageResponseTimeThreshold&quot;));
String y =JMeterUtils.getProperty(&quot;averegareResponsTimeCalculator&quot;);      
String z = vars.get(&quot;threadCount&quot;); 
int averegareResponsTimeCalculator= Integer.valueOf(y)/Integer.valueOf(z); 
int failedResponseCount = Integer.valueOf(JMeterUtils.getProperty(&quot;failedResponseCounter&quot;));     
boolean isFailedResponseAboveThreshold = false;
boolean isAverageResponseTimeAboveThreshold = false;
boolean istTestPlanPass = false;
f = new FileOutputStream(&quot;results.txt&quot;, false);
p = new PrintStream(f); 

if(failedResponseCount &gt; failedResponseThreshold){
	p.println(&quot;Test Plan: Response Validation: Actual failedResponseCount &gt; Expected ? (&quot;+failedResponseCount+&quot; &gt; &quot;+failedResponseThreshold+&quot; ?) = FAIL&quot;);
}else{
	p.println(&quot;Test Plan: Response Validation: Actual failedResponseCount &gt; Expected ? (&quot;+failedResponseCount+&quot; &gt; &quot;+failedResponseThreshold+&quot; ?) =  PASS&quot;);
	isFailedResponseAboveThreshold = true;
	}

if(averegareResponsTimeCalculator &gt; averageResponseTimeThreshold){
	p.println(&quot;Test Plan: Average Response Time Validation: Actual averegareResponsTime &gt; Expected ? (&quot;+averegareResponsTimeCalculator+&quot; &gt; &quot;+averageResponseTimeThreshold+&quot; ?) = FAIL&quot;);
}else{
	p.println(&quot;Test Plan: Average Response Time Validation: Actual averegareResponsTime &gt; Expected ? (&quot;+averegareResponsTimeCalculator+&quot; &gt; &quot;+averageResponseTimeThreshold+&quot; ?) =  PASS&quot;);
	isAverageResponseTimeAboveThreshold = true;
	}

if (isFailedResponseAboveThreshold == true &amp;&amp; isAverageResponseTimeAboveThreshold == true) {
	istTestPlanPass = true;
}

p.println(&quot;Test Plan: &quot; + (istTestPlanPass ? &quot;PASS&quot; : &quot;FAIL&quot;));
log.info(&quot;DONE----------------------------&quot;);

p.close();
f.close();</stringProp>
