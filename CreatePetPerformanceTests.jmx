          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor_failedResponseCounter" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import org.apache.jmeter.util.JMeterUtils;

String y = JMeterUtils.getProperty(&quot;failedResponseCounter&quot;);
int failedResponseCount = 0;

failedResponseCount = Integer.valueOf(y);
if (!prev.getResponseMessage().equals(&quot;OK&quot;)){
	failedResponseCount++;
	JMeterUtils.setProperty(&quot;failedResponseCounter&quot;, String.valueOf(failedResponseCount));
}
</stringProp>
