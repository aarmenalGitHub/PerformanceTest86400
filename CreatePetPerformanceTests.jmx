          <BeanShellPostProcessor guiclass="TestBeanGUI" testclass="BeanShellPostProcessor" testname="BeanShell PostProcessor_averegareResponsTimeCalculator" enabled="true">
            <boolProp name="resetInterpreter">false</boolProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="script">import org.apache.jmeter.util.JMeterUtils;

int averegareResponsTimeCalculator = Integer.valueOf(JMeterUtils.getProperty(&quot;averegareResponsTimeCalculator&quot;));
long sampleStartTime=prev.getStartTime();
long sampleEndTime=prev.getEndTime();
long sampleLoadTime=sampleEndTime - sampleStartTime;

averegareResponsTimeCalculator= averegareResponsTimeCalculator + (int)sampleLoadTime;
JMeterUtils.setProperty(&quot;averegareResponsTimeCalculator&quot;, String.valueOf(averegareResponsTimeCalculator));


</stringProp>
